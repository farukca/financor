<div id="finpoints_list_div" class="table-responsive">
	<table class="table table-striped table-bordered table-hover" id="finpoints_list_table">
	  <thead>
	    <tr>
		  	<th class="col-md-2"><%= t("simple_form.labels.finpoint.title") %></th>
		  	<th class="col-md-2"><%= t("simple_form.labels.finpoint.point_type") %></th>
		  	<th class="col-md-1"><%= t("simple_form.labels.finpoint.curr") %></th>
		  	<th class="col-md-3"><%= t("simple_form.labels.finpoint.reference") %></th>
		  	<th class="col-md-1"><%= t("simple_form.labels.finpoint.bank") %></th>
		  	<th class="col-md-1"><%= t("simple_form.labels.finpoint.branch") %></th>
		  	<th class="col-md-2"><%= t("simple_form.labels.finpoint.manager_id") %></th>
	  	</tr>
	  </thead>
	  <tbody class="report-body">
	 	<% @finpoints.each do |finpoint| %>
	     <tr>
	        <td><%= link_to finpoint.title, finpoint  %></td>
	        <td><%= t("simple_form.options.finpoint.point_type.#{finpoint.point_type}") %></td>
	        <td><%= finpoint.curr %></td>
	        <td><%= finpoint.reference %></td>
	        <td><%= finpoint.bank %></td>
	        <td><%= finpoint.branch %></td>
	        <td><%= finpoint.manager %></td>
	      </tr>
	    <% end %>
	  </tbody>
	 </table>

	<% if objects.respond_to? :current_page %>
	  <ul class="pagination">
	   	<%= paginate objects, remote: true %>
	  </ul>
	<% end %>
</div>