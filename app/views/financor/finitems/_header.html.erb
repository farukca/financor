<div id="finitem_#{finitem.id}_profile_header" class="">

  <div class="col-md-1 col-xs-3 col-sm-3">
    <i class="fa fa-building-o fa-5x"></i>
  </div>

  <div class="col-md-11 col-xs-9 col-sm-9">
    <div class="row">
      <div class="col-md-10 profile-info">
        <h1>
          <%= finitem.code %>
        </h1>
        <p>
          <i class="fa fa-pencil"></i>
		  <%= t("simple_form.options.finitem.item_type.#{finitem.item_type}") %>
        </p>
      </div>
      <div class="col-md-1">
  	      <%= render "actions", finitem: finitem %>
      </div>
    </div>
    <hr>
  </div>

</div>