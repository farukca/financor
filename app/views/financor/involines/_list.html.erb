<div id="involines_list_div" class="table-responsive">
	<table class="table table-striped table-bordered table-hover" id="involines_list_table">
		<thead>
      <tr>
        <th style="width: 20%"><%= t("simple_form.labels.involine.name") %></th>
        <th style="width: 25%"><%= t("simple_form.labels.involine.notes") %></th>
        <th style="width: 10%"><%= t("simple_form.labels.involine.unit_number") %></th>
        <th style="width: 10%"><%= t("simple_form.labels.involine.unit_price") %></th>
        <th style="width: 10%"><%= t("simple_form.labels.involine.tax") %></th>
        <th style="width: 15%"><%= t("simple_form.labels.involine.total_amount") %></th>
        <th style="width: 5%" class="hidden-print">#</th>
      </tr>
    </thead>
	  <tbody id="invoice_<%= @invoice.id %>_involines">
	    <%= render objects %>
	  </tbody>
	 </table>

	<% if objects.respond_to? :current_page %>
	  <ul class="pagination">
	   	<%= paginate objects, remote: true %>
	  </ul>
	<% end %>
</div>