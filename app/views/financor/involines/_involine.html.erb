<tr id="involine_<%= involine.id %>_div">
  <td><%= involine.name %></td>
  <td><%= involine.notes %></td>
	<td><%= involine.unit_number %></td>
	<td><%= t("simple_form.options.involine.unit_type.#{involine.unit_type}") %></td>
	<td><%= involine.unit_price %></td>
	<td><%= involine.vat_amount %></td>
	<td><%= involine.total_amount %></td>
	<td class="hidden-print">
			<%= link_to financor.edit_invoice_involine_path(involine.invoice, involine), remote: true, title: t("simple_form.helpers.defaults.edit") do %>
			  <i class="fa fa-edit"></i>
			<% end %>		
			<%= link_to [involine.invoice, involine], method: :delete, remote: true, data: { confirm: t("simple_form.messages.defaults.destroy_confirm") }, title: t("simple_form.helpers.defaults.delete") do %>
			  <i class="fa fa-trash-o"></i>
			<% end %>
	</td>
</tr>