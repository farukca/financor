<tr id="involine_<%= involine.id %>_div">
  <td><%= involine.name %></td>
  <td><%= company_name(involine.company) %></td>
  <td><%= t("simple_form.options.involine.debit_credit.#{involine.debit_credit}") %></td>
	<td><%= involine.unit_number %> <%= t("simple_form.options.involine.unit_type.#{involine.unit_type}") %></td>
	<td><%= involine.unit_price %></td>
	<td><%= involine.curr %></td>
	<td><%= involine.vat_amount %></td>
	<td><%= involine.total_amount %></td>
	<td class="hidden-print">
			<%= link_to financor.edit_involine_path(involine), remote: true, title: t("simple_form.helpers.defaults.edit") do %>
			  <i class="fa fa-edit"></i>
			<% end %>		
			<%= link_to financor.involine_path(involine), method: :delete, remote: true, data: { confirm: t("simple_form.messages.defaults.destroy_confirm") }, title: t("simple_form.helpers.defaults.delete") do %>
			  <i class="fa fa-trash-o"></i>
			<% end %>
	</td>
</tr>	    